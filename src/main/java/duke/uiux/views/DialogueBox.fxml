<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>

<fx:root alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" 
    prefWidth="${dialogue.prefWidth}" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
    spacing="20.0">
  <children>
    <Label fx:id="dialogue" text="Label" wrapText="true" />
    <Circle fx:id="displayPicture" radius="25.0"/>
  </children>
  <padding>
    <Insets bottom="15.0" left="10.0" right="10.0" top="15.0" />
  </padding>
</fx:root>