    <children>
        <Label fx:id="dialog" text="Label" wrapText="true">
         <HBox.margin>
            <Insets right="8.0" />
         </HBox.margin></Label>
      <Pane prefHeight="200.0" prefWidth="200.0" style="-fx-border-radius: 9999px; -fx-pref-width: 80px; -fx-pref-height: 80px;">
