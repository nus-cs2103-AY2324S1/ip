<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" alignment="CENTER" minHeight="-Infinity" style="-fx-background-color: #254f77; -fx-background-radius: 20;" text="Label" textFill="WHITE" wrapText="true">
            <padding>
                <Insets bottom="5.0" left="10" right="10.0" top="5.0" />
            </padding>
            <HBox.margin>
                <Insets left="10" right="10.0" top="10"/>
            </HBox.margin>
        </Label>
        <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true">
            <HBox.margin>
                <Insets left="20.0" right="5.0" />
            </HBox.margin></ImageView>
    </children>
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
    <padding>
        <Insets bottom="20.0" />
    </padding>
</fx:root>
