<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>


<?import javafx.scene.layout.VBox?>
<fx:root alignment="TOP_RIGHT" maxHeight="Infinity" maxWidth="1.7976931348623157E308" prefWidth="400.0"
         type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Pane fx:id="messageOutline" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="39.0"
              prefWidth="400.0" styleClass="gray-background" stylesheets="@../css/Application.css">
            <children>
                <Label fx:id="dialog" text="Label with long text that should wrap to the next line when needed." wrapText="true">
                <opaqueInsets>
                    <Insets />
                </opaqueInsets>
                <font>
                    <Font name="Segoe UI" size="14.0" />
                </font>
                <padding>
                    <Insets bottom="15.0" left="12.0" right="10.0" top="12.0" />
                </padding>
                </Label>
            </children>
            <HBox.margin>
                <Insets />
            </HBox.margin>
        </Pane>
        <Circle fx:id="circle" fill="#007df2" radius="23.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0">
            <HBox.margin>
                <Insets left="10.0" right="10.0" />
            </HBox.margin>
        </Circle>
    </children>
    <padding>
        <Insets bottom="15.0" left="10.0" right="10.0" top="15.0" />
    </padding>
</fx:root>


