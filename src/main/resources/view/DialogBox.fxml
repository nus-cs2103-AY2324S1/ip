<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<fx:root alignment="TOP_RIGHT" maxHeight="Infinity" maxWidth="Infinity"
         type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/17"
         xmlns:fx="http://javafx.com/fxml/1" spacing="10.0">
    <children>
        <Label fx:id="dialog" text="Label" wrapText="true" />
        <ImageView fx:id="displayPicture" fitHeight="65.0d" fitWidth="65.0d" preserveRatio="true" />
    </children>
    <padding>
        <Insets bottom="20.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
