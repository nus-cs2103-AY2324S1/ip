<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
         style="-fx-padding: 12;" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/20.0.1"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" minHeight="-Infinity"
               style="-fx-padding: 12; -fx-background-color: #ffffc0; -fx-background-radius: 12;" text="Label"
               textAlignment="JUSTIFY" wrapText="true">
            <font>
                <Font name="American Typewriter" size="13.0"/>
            </font>
        </Label>
        <ImageView fx:id="displayPicture" fitHeight="52.0" fitWidth="52.0" pickOnBounds="true" preserveRatio="true">
            <HBox.margin>
                <Insets left="8.0" right="8.0"/>
            </HBox.margin>
        </ImageView>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0"/>
    </padding>
</fx:root>
