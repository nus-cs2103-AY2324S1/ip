<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" minHeight="-Infinity" minWidth="-Infinity" style="-fx-background-color: #0E59C7; -fx-background-radius: 0 12 12 12;" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" alignment="TOP_LEFT" text="Label" textFill="WHITE" wrapText="true" VBox.vgrow="ALWAYS">
            <font>
                <Font name="Tahoma" size="14.0" />
            </font>
            <VBox.margin>
                <Insets />
            </VBox.margin>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </Label>
    </children>
    <padding>
        <Insets bottom="8.0" left="8.0" right="12.0" top="8.0" />
    </padding>
</fx:root>
